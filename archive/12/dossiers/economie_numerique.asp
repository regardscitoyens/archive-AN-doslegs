          <tr>
            <td WIDTH="89" VALIGN="TOP"><font FACE="Arial" SIZE="3">article 20</font></td>
            <td WIDTH="621" VALIGN="TOP"><font FACE="Arial" SIZE="3">responsabilité civile de ces prestataires</font></td>
          </tr>
          <tr>
            <td WIDTH="89" VALIGN="TOP"><font FACE="Arial" SIZE="3">article 21</font></td>
            <td WIDTH="621" VALIGN="TOP"><font FACE="Arial" SIZE="3">responsabilité civile attachée à la certification</font></td>
